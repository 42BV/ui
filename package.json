{
  "name": "@42.nl/ui",
  "version": "3.19.2",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "React components as used in 42.nl products.",
  "repository": {
    "type": "git",
    "url": "https://github.com/42BV/ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*",
    "!**/*.test.d.ts",
    "!**/*.test.d.ts",
    "!**/*.stories.d.ts",
    "!dist/test"
  ],
  "license": "SEE LICENSE IN LICENSE.MD",
  "scripts": {
    "start": "start-storybook -c storybook",
    "clean": "rm -rf dist",
    "test": "npm run lint && npm run test:ts && npm run test:coverage",
    "test:ts": "tsc --version && tsc --noEmit",
    "test:coverage": "jest test --coverage --no-cache",
    "test:watch": "jest --watch --coverage",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "storybook:build": "build-storybook -c storybook -o docs/storybook && git add docs && git clean -df",
    "docs": "jekyll serve --source docs",
    "release": "np --otp",
    "compile": "npm run clean && rollup -c",
    "dev:publish": "./scripts/dev-publish.sh",
    "version": "npm run compile && jekyll build && npm run storybook:build && git add stats.html && git add .size-snapshot.json",
    "better-audit": "node node_modules/better-npm-audit audit"
  },
  "dependencies": {
    "@tippyjs/react": "4.2.6",
    "bootstrap": "5.1.3",
    "moment": "2.29.2",
    "overlayscrollbars": "1.13.1",
    "overlayscrollbars-react": "0.2.3",
    "pica": "9.0.1",
    "react-avatar-editor": "12.0.0",
    "react-bootstrap-typeahead": "5.2.2",
    "react-color": "2.19.3",
    "react-datetime": "3.1.1",
    "react-display-name": "0.2.5",
    "react-final-form": "6.5.9",
    "react-is": "18.0.0",
    "react-quill": "^1.3.5",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "8.3.3",
    "tippy.js": "6.3.7"
  },
  "peerDependencies": {
    "@42.nl/jarb-final-form": "^2.0.4",
    "@42.nl/react-error-store": "^1.1.1",
    "@42.nl/spring-connect": "^4.3.0",
    "classnames": "^2.3.1",
    "draft-convert": "^2.1.12",
    "draft-js": "^0.11.7",
    "final-form": "^4.20.4",
    "lodash": "^4.17.21",
    "react": "^17.0.2||^18.0.0",
    "react-dom": "^17.0.2||^18.0.0",
    "react-draft-wysiwyg": "^1.14.7",
    "react-router-dom": "^5.3.0||^6.0.0",
    "reactstrap": "^9.0.1"
  },
  "devDependencies": {
    "@42.nl/jarb-final-form": "2.1.2",
    "@42.nl/react-error-store": "1.1.3",
    "@42.nl/spring-connect": "5.0.2",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-docs": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/react": "6.4.22",
    "@testing-library/react-hooks": "8.0.0",
    "@types/classnames": "2.3.1",
    "@types/enzyme": "3.10.12",
    "@types/enzyme-to-json": "1.5.4",
    "@types/estree": "0.0.51",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.181",
    "@types/overlayscrollbars": "1.12.1",
    "@types/react": "17.0.44",
    "@types/react-avatar-editor": "12.0.0",
    "@types/react-bootstrap-typeahead": "5.1.8",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "17.0.15",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.4",
    "better-npm-audit": "^3.7.3",
    "classnames": "2.3.1",
    "draft-convert": "^2.1.12",
    "draft-js": "^0.11.7",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.13.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "final-form": "4.20.6",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.3.7",
    "lodash": "4.17.21",
    "np": "^7.6.1",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-async": "10.0.1",
    "react-docgen-typescript-plugin": "1.0.1",
    "react-dom": "17.0.2",
    "react-draft-wysiwyg": "^1.14.7",
    "react-query": "3.34.19",
    "react-router-dom": "6.3.0",
    "react-test-renderer": "17.0.2",
    "reactstrap": "9.0.2",
    "rollup": "2.70.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-typescript2": "0.31.2",
    "rollup-plugin-visualizer": "5.6.0",
    "sass": "1.50.0",
    "sass-loader": "12.6.0",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0"
  },
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "<rootDir>/../setupTests.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/**/*.{ts,tsx}",
      "!<rootDir>/index.ts",
      "!<rootDir>/form/story-utils.tsx",
      "!<rootDir>/**/*.stories.tsx",
      "!<rootDir>/test/**/*"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "restoreMocks": true,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,jsx,json,scss,ts,tsx}": [
      "prettier --single-quote --trailing-comma none --write"
    ]
  }
}
