{
  "name": "@42.nl/ui",
  "version": "4.0.6",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "React components as used in 42.nl products.",
  "repository": {
    "type": "git",
    "url": "https://github.com/42BV/ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*",
    "!**/*.test.d.ts",
    "!**/*.test.d.ts",
    "!**/*.stories.d.ts",
    "!dist/test"
  ],
  "license": "SEE LICENSE IN LICENSE.MD",
  "scripts": {
    "start": "start-storybook -c storybook",
    "clean": "rm -rf dist",
    "test": "npm run lint && npm run test:ts && npm run test:coverage",
    "test:ts": "tsc --version && tsc --noEmit",
    "test:coverage": "TZ=UTC jest test --coverage --no-cache",
    "test:watch": "TZ=UTC jest --watch --coverage",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "storybook:build": "build-storybook -c storybook -o docs/storybook && git add docs && git clean -df",
    "docs": "jekyll serve --source docs",
    "release": "np --otp",
    "compile": "npm run clean && rollup -c",
    "dev:publish": "./scripts/dev-publish.sh",
    "version": "npm run compile && jekyll build && npm run storybook:build && git add stats.html && git add .size-snapshot.json",
    "better-audit": "node node_modules/better-npm-audit audit"
  },
  "dependencies": {
    "bootstrap": "5.2.0",
    "react-is": "18.2.0",
    "reactstrap": "9.1.2"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2||^18.0.0"
  },
  "optionalDependencies": {
    "@42.nl/jarb-final-form": "^2.0.4",
    "@42.nl/react-error-store": "^1.1.1",
    "@42.nl/spring-connect": "^4.3.0||^5.1.0",
    "@tippyjs/react": "4.2.6",
    "final-form": "^4.20.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "overlayscrollbars": "^1.13.2",
    "overlayscrollbars-react": "^0.3.0",
    "pica": "^9.0.1",
    "react-avatar-editor": "13.0.0",
    "react-bootstrap-typeahead": "5.2.2",
    "react-color": "^2.19.3",
    "react-datetime": "^3.1.1",
    "react-display-name": "^0.2.5",
    "react-final-form": "^6.5.9",
    "react-quill": "^1.3.5",
    "react-router-dom": "^5.3.0||^6.0.0",
    "react-text-mask": "^5.4.3",
    "react-textarea-autosize": "^8.3.4",
    "tippy.js": "6.3.7"
  },
  "devDependencies": {
    "@42.nl/jarb-final-form": "2.1.4",
    "@42.nl/react-error-store": "1.1.3",
    "@42.nl/spring-connect": "5.1.0",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-docs": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@testing-library/dom": "8.16.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@tippyjs/react": "4.2.6",
    "@types/classnames": "2.3.1",
    "@types/estree": "1.0.0",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/overlayscrollbars": "1.12.1",
    "@types/react": "18.0.15",
    "@types/react-avatar-editor": "12.0.0",
    "@types/react-bootstrap-typeahead": "5.1.8",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "better-npm-audit": "3.7.3",
    "canvas": "2.9.3",
    "classnames": "2.3.1",
    "eslint": "8.20.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "final-form": "4.20.7",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-watch-typeahead": "2.0.0",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "np": "7.6.2",
    "overlayscrollbars": "1.13.3",
    "overlayscrollbars-react": "0.3.0",
    "pica": "9.0.1",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-async": "10.0.1",
    "react-avatar-editor": "13.0.0",
    "react-bootstrap-typeahead": "5.2.2",
    "react-color": "2.19.3",
    "react-datetime": "3.1.1",
    "react-display-name": "0.2.5",
    "react-docgen-typescript-plugin": "1.0.1",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-query": "3.39.2",
    "react-quill": "1.3.5",
    "react-router-dom": "6.3.0",
    "react-test-renderer": "18.2.0",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "8.3.4",
    "rollup": "2.77.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-typescript2": "0.32.1",
    "rollup-plugin-visualizer": "5.7.1",
    "sass": "1.54.0",
    "sass-loader": "13.0.2",
    "tippy.js": "6.3.7",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0"
  },
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "<rootDir>/../setupTests.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/**/*.{ts,tsx}",
      "!<rootDir>/index.ts",
      "!<rootDir>/form/story-utils.tsx",
      "!<rootDir>/**/*.stories.tsx",
      "!<rootDir>/test/**/*"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "restoreMocks": true,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,jsx,json,scss,ts,tsx}": [
      "prettier --single-quote --trailing-comma none --write"
    ]
  }
}
