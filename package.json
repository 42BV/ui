{
  "name": "@42.nl/ui",
  "version": "3.16.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "React components as used in 42.nl products.",
  "repository": {
    "type": "git",
    "url": "https://github.com/42BV/ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*",
    "!**/*.test.d.ts",
    "!**/*.test.d.ts",
    "!**/*.stories.d.ts",
    "!dist/test"
  ],
  "license": "SEE LICENSE IN LICENSE.MD",
  "scripts": {
    "start": "start-storybook -c storybook",
    "clean": "rm -rf dist",
    "test": "npm run lint && npm run test:ts && npm run test:coverage",
    "test:ts": "tsc --version && tsc --noEmit",
    "test:coverage": "jest test --coverage --no-cache",
    "test:watch": "jest --watch --coverage",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "storybook:build": "build-storybook -c storybook -o docs/storybook && git add docs && git clean -df",
    "docs": "jekyll serve --source docs",
    "release": "np --otp",
    "compile": "npm run clean && rollup -c",
    "dev:publish": "./scripts/dev-publish.sh",
    "version": "npm run compile && jekyll build && npm run storybook:build && git add stats.html && git add .size-snapshot.json"
  },
  "dependencies": {
    "bootstrap": "4.5.3",
    "material-design-icons": "3.0.1",
    "moment": "2.29.1",
    "overlayscrollbars": "1.13.0",
    "overlayscrollbars-react": "0.2.2",
    "pica": "6.1.1",
    "tippy.js": "6.2.7",
    "@tippyjs/react": "4.2.0",
    "react-avatar-editor": "11.0.13",
    "react-bootstrap-typeahead": "5.1.2",
    "react-color": "2.19.3",
    "react-datetime": "3.0.4",
    "react-display-name": "0.2.5",
    "react-final-form": "6.5.2",
    "react-is": "17.0.1",
    "react-quill": "1.3.5",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "8.3.0"
  },
  "peerDependencies": {
    "@42.nl/jarb-final-form": "^2.0.0",
    "@42.nl/react-error-store": "^1.1.0",
    "@42.nl/spring-connect": "^4.2.1",
    "classnames": "^2.2.6",
    "final-form": "^4.20.1",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.7.1"
  },
  "devDependencies": {
    "@42.nl/jarb-final-form": "2.0.0",
    "@42.nl/react-error-store": "1.1.0",
    "@42.nl/spring-connect": "4.2.1",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@storybook/addon-actions": "6.1.10",
    "@storybook/addon-docs": "6.1.10",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.1.10",
    "@testing-library/react-hooks": "3.6.0",
    "@types/classnames": "2.2.11",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-to-json": "1.5.4",
    "@types/jest": "26.0.16",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.10",
    "@types/overlayscrollbars": "1.12.0",
    "@types/react": "17.0.0",
    "@types/react-avatar-editor": "10.3.5",
    "@types/react-bootstrap-typeahead": "5.1.0",
    "@types/react-color": "3.0.4",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/webpack": "4.41.25",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "classnames": "2.2.6",
    "css-loader": "5.0.1",
    "enzyme": "3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.3.2",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-jest": "24.1.3",
    "final-form": "4.20.1",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "lodash": "4.17.21",
    "node-sass": "5.0.0",
    "np": "7.0.0",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-async": "10.0.1",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "17.0.1",
    "reactstrap": "8.7.1",
    "rollup": "2.34.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-visualizer": "4.2.0",
    "sass": "1.30.0",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2"
  },
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "<rootDir>/../setupTests.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/**/*.{ts,tsx}",
      "!<rootDir>/index.ts",
      "!<rootDir>/form/story-utils.tsx",
      "!<rootDir>/**/*.stories.tsx",
      "!<rootDir>/test/**/*"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "restoreMocks": true,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,jsx,json,scss,ts,tsx}": [
      "prettier --single-quote --trailing-comma none --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
