{
  "name": "@42.nl/ui",
  "version": "2.4.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "React components as used in 42.nl products.",
  "repository": {
    "type": "git",
    "url": "https://github.com/42BV/ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "license": "SEE LICENSE IN LICENSE.MD",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run storybook",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "test-all": "npm run lint && jest --coverage",
    "build": "npm run clean && rollup -c",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "storybook:build": "build-storybook -c storybook -o docs/storybook",
    "storybook": "start-storybook -c storybook",
    "docs": "jekyll serve --source docs",
    "release": "np --otp",
    "dev:publish": "./scripts/dev-publish.sh",
    "prepublishOnly": "npm run clean && npm run test-all && npm run build",
    "version": "jekyll build && npm run storybook:build && git add docs"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "material-design-icons": "3.0.1",
    "moment": "2.24.0",
    "overlayscrollbars": "1.10.0",
    "overlayscrollbars-react": "0.2.0",
    "pica": "5.1.0",
    "react-avatar-editor": "12.0.0-beta.0",
    "react-bootstrap-typeahead": "4.0.0-alpha.6",
    "react-datetime": "2.16.3",
    "react-display-name": "0.2.4",
    "react-final-form": "6.3.0",
    "react-is": "16.11.0",
    "react-quill": "1.3.3",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "7.1.2"
  },
  "peerDependencies": {
    "@42.nl/jarb-final-form": "^1.1.0",
    "@42.nl/react-error-store": "^1.0.1",
    "@42.nl/spring-connect": "^4.1.0",
    "classnames": "^2.2.6",
    "final-form": "^4.18.5",
    "lodash": "^4.17.15",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.0",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@42.nl/jarb-final-form": "1.1.0",
    "@42.nl/react-error-store": "1.0.1",
    "@42.nl/spring-connect": "4.1.0",
    "@babel/plugin-transform-modules-commonjs": "7.7.0",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-docs": "5.2.5",
    "@storybook/preset-scss": "1.0.2",
    "@storybook/preset-typescript": "1.1.0",
    "@storybook/react": "5.2.5",
    "@testing-library/react-hooks": "3.2.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/enzyme-to-json": "1.5.3",
    "@types/jest": "24.0.22",
    "@types/lodash": "4.14.145",
    "@types/node": "12.12.6",
    "@types/overlayscrollbars": "1.9.0",
    "@types/react": "16.9.11",
    "@types/react-avatar-editor": "10.3.5",
    "@types/react-bootstrap-typeahead": "3.4.5",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.2",
    "@types/reactstrap": "8.0.6",
    "@types/storybook__react": "4.0.2",
    "@types/webpack": "4.39.8",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "classnames": "2.2.6",
    "css-loader": "3.2.0",
    "danger": "9.2.4",
    "danger-plugin-jest": "1.3.0",
    "danger-plugin-yarn": "1.3.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.6.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "final-form": "4.18.6",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "node-sass": "4.13.0",
    "np": "5.1.2",
    "react": "16.11.0",
    "react-async": "9.0.0",
    "react-docgen-typescript-loader": "3.3.0",
    "react-dom": "16.11.0",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.11.0",
    "reactstrap": "8.1.1",
    "rollup": "1.26.3",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-size-snapshot": "0.10.0",
    "rollup-plugin-typescript2": "0.25.2",
    "sass": "1.23.3",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/src/index.ts",
      "!<rootDir>/src/form/story-utils.tsx",
      "!<rootDir>/src/**/*.stories.tsx",
      "!<rootDir>/src/test/**/*",
      "!<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  }
}
