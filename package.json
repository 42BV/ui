{
  "name": "@42.nl/ui",
  "version": "2.19.1",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "React components as used in 42.nl products.",
  "repository": {
    "type": "git",
    "url": "https://github.com/42BV/ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "license": "SEE LICENSE IN LICENSE.MD",
  "scripts": {
    "tsc": "tsc --version && tsc --noEmit",
    "clean": "rm -rf dist",
    "start": "npm run storybook",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "test-all": "npm run lint && npm run tsc && jest --coverage --no-cache",
    "build": "npm run clean && rollup -c",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "storybook:build": "build-storybook -c storybook -o docs/storybook",
    "storybook": "start-storybook -c storybook",
    "docs": "jekyll serve --source docs",
    "release": "np --otp",
    "dev:publish": "./scripts/dev-publish.sh",
    "version": "npm run clean && npm run test-all && npm run build && jekyll build && npm run storybook:build && git add stats.html && git add .size-snapshot.json && git add docs && git clean -df"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "material-design-icons": "3.0.1",
    "moment": "2.24.0",
    "overlayscrollbars": "1.10.3",
    "overlayscrollbars-react": "0.2.1",
    "pica": "5.1.0",
    "tippy.js": "6.2.0",
    "@tippyjs/react": "4.0.2",
    "react-avatar-editor": "11.0.7",
    "react-bootstrap-typeahead": "4.0.0-alpha.6",
    "react-color": "2.18.0",
    "react-datetime": "2.16.3",
    "react-display-name": "0.2.5",
    "react-final-form": "6.3.5",
    "react-is": "16.12.0",
    "react-quill": "1.3.3",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "7.1.2"
  },
  "peerDependencies": {
    "@42.nl/jarb-final-form": "^1.2.0",
    "@42.nl/react-error-store": "^1.0.1",
    "@42.nl/spring-connect": "^4.1.0",
    "classnames": "^2.2.6",
    "final-form": "^4.18.5",
    "lodash": "^4.17.15",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.0",
    "reactstrap": "^8.3.2"
  },
  "devDependencies": {
    "@42.nl/jarb-final-form": "1.2.0",
    "@42.nl/react-error-store": "1.0.1",
    "@42.nl/spring-connect": "4.1.0",
    "@babel/plugin-transform-modules-commonjs": "7.8.3",
    "@storybook/addon-actions": "5.3.13",
    "@storybook/addon-docs": "5.3.13",
    "@storybook/preset-scss": "1.0.2",
    "@storybook/react": "5.3.13",
    "@testing-library/react-hooks": "3.2.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/enzyme-to-json": "1.5.3",
    "@types/jest": "25.1.2",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.1",
    "@types/overlayscrollbars": "1.9.0",
    "@types/react": "16.9.19",
    "@types/react-avatar-editor": "10.3.5",
    "@types/react-bootstrap-typeahead": "3.4.5",
    "@types/react-color": "3.0.1",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/reactstrap": "8.4.1",
    "@types/webpack": "4.41.6",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "classnames": "2.2.6",
    "css-loader": "3.4.2",
    "danger": "9.2.10",
    "danger-plugin-jest": "1.3.0",
    "danger-plugin-yarn": "1.3.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "final-form": "4.18.7",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "husky": "4.2.2",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "lodash": "4.17.15",
    "node-sass": "4.13.1",
    "np": "6.2.4",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-async": "10.0.0",
    "react-docgen-typescript-loader": "3.6.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.12.0",
    "reactstrap": "8.4.1",
    "rollup": "1.31.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-size-snapshot": "0.11.0",
    "rollup-plugin-typescript2": "0.26.0",
    "rollup-plugin-visualizer": "3.3.1",
    "sass": "1.25.1-test.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5"
  },
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "<rootDir>/../setupTests.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/**/*.{ts,tsx}",
      "!<rootDir>/index.ts",
      "!<rootDir>/form/story-utils.tsx",
      "!<rootDir>/**/*.stories.tsx",
      "!<rootDir>/test/**/*"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "restoreMocks": true
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,jsx,json,scss,ts,tsx}": [
      "prettier --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
