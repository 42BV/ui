{
  "name": "@42.nl/ui",
  "version": "1.1.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "React components as used in 42.nl products.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run storybook",
    "test": "jest --watch",
    "test-all": "npm run lint && jest --coverage",
    "build": "npm run clean && rollup -c",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:build": "build-storybook -c storybook -o docs/storybook",
    "storybook": "start-storybook -c storybook",
    "docs": "jekyll serve --source docs",
    "release": "np --otp",
    "dev:publish": "./scripts/dev-publish.sh",
    "prepublishOnly": "npm run clean && npm run test-all && npm run build",
    "version": "jekyll build && npm run storybook:build"
  },
  "dependencies": {
    "@42.nl/jarb-final-form": "1.0.0",
    "@42.nl/react-error-store": "1.0.1",
    "@42.nl/spring-connect": "4.0.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "final-form": "4.18.5",
    "lodash": "4.17.15",
    "material-design-icons": "3.0.1",
    "moment": "2.24.0",
    "pica": "5.1.0",
    "react-avatar-editor": "11.0.7",
    "react-bootstrap-typeahead": "4.0.0-alpha.6",
    "react-datetime": "2.16.3",
    "react-display-name": "0.2.4",
    "react-final-form": "6.3.0",
    "react-quill": "1.3.3",
    "react-router-dom": "5.0.1",
    "react-text-mask": "5.4.3",
    "react-textarea-autosize": "7.1.0",
    "reactstrap": "8.0.1"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@storybook/addon-actions": "5.2.1",
    "@storybook/addon-docs": "5.2.1",
    "@storybook/preset-scss": "1.0.2",
    "@storybook/preset-typescript": "1.1.0",
    "@storybook/react": "5.2.1",
    "@storybook/storybook-deployer": "2.8.1",
    "@testing-library/react-hooks": "2.0.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/enzyme-to-json": "1.5.3",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-avatar-editor": "10.3.4",
    "@types/react-bootstrap-typeahead": "3.4.5",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/reactstrap": "8.0.4",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "danger": "9.2.0",
    "danger-plugin-jest": "1.3.0",
    "danger-plugin-yarn": "1.3.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.4.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "fs-extra": "8.1.0",
    "glob": "7.1.4",
    "jest": "24.9.0",
    "lerna": "3.16.4",
    "node-sass": "4.12.0",
    "np": "5.1.0",
    "react-async": "8.0.0",
    "react-docgen-typescript-loader": "3.2.1",
    "react-i18next": "10.12.5",
    "react-test-renderer": "16.9.0",
    "rollup": "1.21.4",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-size-snapshot": "0.10.0",
    "rollup-plugin-typescript2": "0.24.2",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.1.1",
    "typescript": "3.6.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/src/index.ts",
      "!<rootDir>/src/form/story-utils.tsx",
      "!<rootDir>/src/**/*.stories.tsx",
      "!<rootDir>/src/test/**/*",
      "!<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  }
}
